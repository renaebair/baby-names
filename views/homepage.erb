<h2>Welcome to NamingTogether!</h2>


<h3>Lists</h3>

<div class='lists'>
  <% List.all.each do |list| %>
    <span><a href='<%= "lists/#{list.url}" %>'><%= list.url %></a></span>
  <% end %>
</div>

<h3>Names</h3>

<div class='lists'>
  <% Name.all.each do |name| %>
    <span><a href='<%= "#" %>'><%= name.name %></a></span>
  <% end %>
</div>
